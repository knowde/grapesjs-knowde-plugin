{
  "name": "grapesjs-knowde-plugin",
  "version": "0.0.3",
  "description": "GrapesJS Knowde Plugin",
  "main": "grapesjs-knowde-plugin.min.js",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "build": "npm run v:patch && webpack --env.production",
    "start": "webpack-dev-server --open --progress --colors"
  },
  "repository": {},
  "keywords": [
    "grapesjs",
    "plugin",
    "wysiwyg"
  ],
  "author": "Knowde",
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "peerDependencies": {
    "grapesjs": "^0.14.52"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "babel-loader": "^8.0.4",
    "eslint": "^5.8.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "1.10.2",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4"
  }
}
